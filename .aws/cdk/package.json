{
  "name": "aws-codepipeline-radiator-aws",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.31.0",
    "@aws-cdk/aws-iam": "^1.31.0",
    "@aws-cdk/aws-lambda": "^1.31.0",
    "@aws-cdk/aws-s3": "^1.31.0",
    "@aws-cdk/aws-s3-deployment": "^1.31.0",
    "@aws-cdk/core": "^1.31.0",
    "@types/node": "^13.11.0",
    "aws-cdk": "^1.31.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "test": "npm-run-all test:*",
    "test:eslint": "eslint --ext .js,.ts .",
    "clean": "rimraf ./dist ./cdk.out",
    "build": "npm run clean && npm run build:tsc",
    "build:tsc": "tsc",
    "deploy": "npm run build && npm run deploy:cdk",
    "deploy:cdk": "cdk deploy --app \"node dist/bin/deploy.js\" --require-approval=never --context env=test"
  }
}
